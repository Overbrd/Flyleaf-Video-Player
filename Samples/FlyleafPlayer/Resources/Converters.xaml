<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
                    xmlns:localConverters="clr-namespace:FlyleafPlayer.Converters"
                    xmlns:fl="clr-namespace:FlyleafLib.Controls.WPF;assembly=FlyleafLib"
                    >

    <fl:StringToRationalConverter x:Key="StringToRational"/>
    <fl:TicksToTimeConverter x:Key="TicksToTime"/>
    <fl:TicksToSecondsConverter x:Key="TicksToSeconds" />
    <fl:TicksToMilliSecondsConverter x:Key="TicksToMilliSeconds"/>
    <fl:BooleanToVisibilityConverter x:Key="BooleanToVisibility"/>

    <localConverters:VolumeToLevelsConverter x:Key="VolumeToLevels"/>
    <localConverters:QualityToLevelsConverter x:Key="QualityToLevels"/>
    <localConverters:SumConverter x:Key="SumConverter"/>
    <localConverters:ColorToBrushConverter x:Key="ColorToBrush"/>
    <localConverters:BrushToColorConverter x:Key="BrushToColor"/>
    
</ResourceDictionary>